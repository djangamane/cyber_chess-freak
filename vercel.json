{
  "builds": [
    {
      "src": "temp-nextjs/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/temp-nextjs/$1"
    }
  ],
  "github": {
    "silent": true
  },
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "devCommand": "next dev --port $PORT",
  "installCommand": "npm install"
}